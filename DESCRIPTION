Package: stagedtrees
Type: Package
Title: Staged Event Trees
Version: 2.2.9999
Authors@R: c(
  person(given = "Gherardo", family = "Varando", role = c("aut", "cre"),
  email = "gherardo.varando@gmail.com",
	comment = c(ORCID = "0000-0002-6708-1103")),
